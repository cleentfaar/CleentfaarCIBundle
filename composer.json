    "keywords":    ["cleentfaar", "profiler", "continuous integration", "bundle"],
    "homepage":    "http://github.com/cleentfaar/CleentfaarCIBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Cas Leentfaar",
            "homepage": "http://github.com/cleentfaar/CleentfaarCIBundle"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "twig/twig": "~1.10"
    },
    "autoload": {
        "psr-0": { "Cleentfaar\\Bundle\\CIBundle": "" }
    },
    "target-dir": "Cleentfaar/Bundle/CIBundle"
